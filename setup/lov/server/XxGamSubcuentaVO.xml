<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="XxGamSubcuentaVO"
   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="xxgam.oracle.apps.fa.sumaria.setup.lov.server.XxGamSubcuentaVORowImpl"
   ComponentClass="xxgam.oracle.apps.fa.sumaria.setup.lov.server.XxGamSubcuentaVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT  B.FLEX_VALUE SUBCUENTA,
             T.DESCRIPTION DESCRIPCION        
FROM FND_FLEX_VALUES_TL T, FND_FLEX_VALUES B, FND_FLEX_VALUE_SETS FVS 
WHERE 1=1
                AND B.FLEX_VALUE_ID = T.FLEX_VALUE_ID 
                AND T.LANGUAGE=USERENV('LANG')
                AND B.FLEX_VALUE_SET_ID = FVS.FLEX_VALUE_SET_ID
                AND FVS.FLEX_VALUE_SET_NAME = 'GAM_GL_SUBCUENTA'
ORDER BY FLEX_VALUE
   ]]></SQLQuery>
