   ComponentClass="xxgam.oracle.apps.fa.sumaria.inquiry.server.XxGamSafSumariaTblVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT  OPERATING_UNIT     
		, PERIOD     
		, START_PERIOD     
		, END_PERIOD     
		, SAF_SUMARIA_ID     
		, XML_STRING
		, LAST_UPDATE_DATE     
		, LAST_UPDATED_BY      
		, LAST_UPDATE_LOGIN     
		, CREATED_BY     
		, CREATION_DATE     
		, REQUEST_ID     
		, PROGRAM_APPLICATION_ID     
		, PROGRAM_ID     
		, PROGRAM_UPDATE_DATE      
		, ATTRIBUTE_CATEGORY     
		, ATTRIBUTE1     
		, ATTRIBUTE2     
		, ATTRIBUTE3     
		, ATTRIBUTE4     
		, ATTRIBUTE5   
FROM	BOLINF.XXGAM_SAF_SUMARIA_TBL
where operating_unit = (select substr(org.NAME,1,2)
FROM hr_all_organization_units org,
         hr_organization_information_v hoi
WHERE org.organization_id = hoi.organization_id
AND  hoi.org_information1 = 'OPERATING_UNIT'
AND  org.ORGANIZATION_ID = fnd_profile.value('ORG_ID')
)
   ]]></SQLQuery>
